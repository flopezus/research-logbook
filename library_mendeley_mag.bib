   journal = {IEEE Transactions on Plasma Science},
   keywords = {Cancer,commercial,high voltage,ion source,medical,neutron generator},
   pages = {2145-2150},
   title = {Novel surface-mounted neutron generator},
   volume = {40},
   year = {2012},
}

