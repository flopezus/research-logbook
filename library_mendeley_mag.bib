@article{Trubnikov1986,
title = {Particle acceleration and neutron production at the necks of plasma pinches},
author = {Trubnikov, B A},
abstractNote = {Induction electric fields are shown to arise in a deuterium plasma pinch because of a redistribution of the current over the discharge cross section. These fields accelerate deuterons, which produce neutrons. The total current I/sub 0/ is assumed to be constant but to consist of two components: I/sub 0/ = I/sub 1/+I/sub 2/, where K/sub 1/(t) is a decaying current in the main pinch, and I/sub 2/(t) is an increasing current in the peripheral plasma around the pinch. The current I/sub 1/ produces fields B = 2I/sub 1//cr and E = I/sub 1/c/sup -2/ ln(r/R)/sup 2/, where R is some length. The current I/sub 1/ is assumed to fall off so rapidly that the deuterons of the peripheral plasma are not ''coiled'' by the magnetic field. They acquire a velocity v = (e/M) ..integral.. E dt = v/sub */ ln(R/r)/sup 2/, where v/sub */ = eI/sub 0//Mc/sup 2/, in an impulsive fashion. The functional dependence r/sup 2/ = R/sup 2/ exp(-v/v/sub */) follows.},
doi = {},
url = {https://www.osti.gov/biblio/7158018}, journal = {Sov. J. Plasma Phys. (Engl. Transl.); (United States)},
number = {} ,
volume = {12:4},
place = {United States},
year = {1986},
month = {4},
}

